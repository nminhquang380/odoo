        <!-- SLIDE - Học liệu-->
        <record id="rule_slide_slide_global" model="ir.rule">
            <field name="name">Slide: always visible (sub rules exist)</field>
            <field name="model_id" ref="model_slide_slide"/>
            <field name="domain_force">[(1, '=', 1)]</field>
        </record>

        <record id="rule_slide_slide_not_website" model="ir.rule">
            <field name="name">Slide: public/portal/user: restricted to published or uploaded by user, and either
                channel member or public channel &amp; (category or previewable)
            </field>
            <field name="model_id" ref="model_slide_slide"/>
            <field name="groups"
                   eval="[(4, ref('base.group_public')), (4, ref('base.group_portal')), (4, ref('base.group_user'))]"/>
            <field name="domain_force">['&amp;',
                '|',
                '&amp;', ('channel_id.visibility', '=', 'public'), '|', ('is_category','=', True), ('is_preview', '=',
                True),
                ('channel_id.partner_ids', '=', user.partner_id.id),
                '&amp;', ('channel_id.website_published', '=', True), '|', ('user_id', '=', user.id),
                ('website_published', '=', True)]
            </field>
            <field name="perm_unlink" eval="0"/>
            <field name="perm_write" eval="0"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
        </record>

        <!-- Rule này cho phép quản trị viên các hình thức đào tạo được CRUD học liệu trên giao diện backend view       -->
        <record id="rule_slide_slide_officer_r" model="ir.rule">
            <field name="name">Slide: QTV read all</field>
            <field name="model_id" ref="model_slide_slide"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups"
                   eval="[(4, ref('group_website_slides_officer'))]"/>
            <field name="perm_unlink" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
        </record>

        <!--        sửa ở đây để giảng viên có thể thêm - sửa  - xóa học liệu (do học liệu kế thừa delegation từ slide_slide-->
        <record id="rule_slide_slide_officer_cw" model="ir.rule">
